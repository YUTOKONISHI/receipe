<p id="notice"><%= notice %></p>

<h1>Allergens Users</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Allergen</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @allergens_users.each do |allergens_user| %>
      <tr>
        <td><%= allergens_user.user_id %></td>
        <td><%= allergens_user.allergen_id %></td>
        <td><%= link_to 'Show', allergens_user %></td>
        <td><%= link_to 'Edit', edit_allergens_user_path(allergens_user) %></td>
        <td><%= link_to 'Destroy', allergens_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Allergens User', new_allergens_user_path %>
